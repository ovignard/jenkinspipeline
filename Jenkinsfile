pipeline {
    agent any
    
    stages{
        stage('Init'){
            steps {
                echo "Testing ..."
                 echo "Testing ..."
                 echo "Testing ..."
            }
        }

        stage('Build'){
            steps {
                echo "Building ..."
            }
        }

        stage('Deploy'){
            steps {
                echo "Code deployed"
            }
        }
          stage('end'){
            steps {
                echo "Code end"
            }
        }
    }
}